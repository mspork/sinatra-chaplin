<div class="listview" style="position:relative;">
<ul>
{{#each items }}
  <a href="#notes/{{id}}">
  <li>
  	<div style="position:absolute;left:0;">
  		<h4>{{ text }}</h4>
  		<p>{{ remindInterval }}</p>
  	</div>
  	<div style="position:absolute;right:0;line-height:80px;">
		  <i class="icon-angle-right icon-2x"></i>		
  	</div>
  </li>
  </a>
{{/each}}
	</ul>
</div> 

